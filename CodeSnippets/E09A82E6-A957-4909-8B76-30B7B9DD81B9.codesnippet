        [_&lt;#ButtonName#&gt; setImage:[UIImage imageNamed:@"&lt;#imageName#&gt;"] forState:UIControlStateHighlighted];
        
        [_&lt;#ButtonName#&gt; addTarget:self action:@selector(&lt;#ButtonName#&gt;Event) forControlEvents:UIControlEventTouchUpInside];
